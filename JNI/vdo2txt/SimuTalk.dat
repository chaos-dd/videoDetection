[Settings]

#以下配置都有默认值，默认情况下不用修改。

#服务器地址，可以是IP，也可以是域名
ServerAddr=

#服务器路径，比如：/webappname/servlet/
ServerPath=

#服务器端口，比如：8080
ServerPort=

#语音服务类型，默认是语音识别，可以支持语音翻译
ServiceType=

#语音合成的语速，0.5 - 2.0之间的一个数值。
SynSpeed=

#模型名称，不要修改
ModelName=

#打开调试日志
EnableLogging=0

#调试日志文件名
LogFileName=

#传输中使用的语音编码
SpeechCodec=

#发送、接受数据的超时时间，毫秒为单位，默认是60000毫秒（60秒）
RecvTimeout=
